    - speaker: {fileID: 11400000, guid: e7f818e81a9ca2940813631374875cef, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 500, y: 0}
      moveSpeed: 2
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "You then told Sheila that Coraline gained back consciousness. Other months
      passed and Coraline grew even bigger. You, Gary, and Sea continued to travel
      across oceans until\u2026"
    speaker: {fileID: 11400000, guid: 1d5042fb7c5b1aa44830898f228f5c62, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 77ffaadeb2c71db4ab2c58f7bf3ed672, type: 2}
