    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "Thanks, Gary\u2026 I\u2019m just a little bit worried. "
    speaker: {fileID: 11400000, guid: 180dd5c3abf0a0c45b2ba75479ffb1d1, type: 2}
    actions:
