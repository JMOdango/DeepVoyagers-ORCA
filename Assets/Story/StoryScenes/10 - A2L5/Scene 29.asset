    - speaker: {fileID: 11400000, guid: 9903c34f3fefc79499d102b04215a59d, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "Oh my\u2026 It\u2019s nice to meet you again, Coraline."
    speaker: {fileID: 11400000, guid: e7f818e81a9ca2940813631374875cef, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 77ffaadeb2c71db4ab2c58f7bf3ed672, type: 2}
