      spriteIndex: 0
      actionType: 1
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "I-it\u2026 talked!"
    speaker: {fileID: 11400000, guid: 77ffaadeb2c71db4ab2c58f7bf3ed672, type: 2}
