      actionType: 0
      coords: {x: 500, y: 0}
      moveSpeed: 2
    - speaker: {fileID: 11400000, guid: 9903c34f3fefc79499d102b04215a59d, type: 2}
      spriteIndex: 1
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "You introduced everyone to Coraline and explained everything that happened.
      Coraline\u2019s clone kept all their memories just like what Sea told you. "
    speaker: {fileID: 11400000, guid: 1d5042fb7c5b1aa44830898f228f5c62, type: 2}
