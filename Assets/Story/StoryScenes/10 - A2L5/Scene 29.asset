      coords: {x: -500, y: 0}
      moveSpeed: 2
    - speaker: {fileID: 11400000, guid: e7f818e81a9ca2940813631374875cef, type: 2}
      spriteIndex: 1
      actionType: 0
      coords: {x: 500, y: 0}
      moveSpeed: 2
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: Were you the ones who saved me?
    speaker: {fileID: 11400000, guid: 9903c34f3fefc79499d102b04215a59d, type: 2}
    actions:
