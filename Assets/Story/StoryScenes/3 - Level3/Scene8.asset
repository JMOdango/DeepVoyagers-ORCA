    - speaker: {fileID: 11400000, guid: e7f818e81a9ca2940813631374875cef, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "So you\u2019re telling me that you need a submarine and a diving suit
      to help this lady over here gain her power?"
    speaker: {fileID: 11400000, guid: 4d3d837e322c27e4887c47353aedbacd, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 4d3d837e322c27e4887c47353aedbacd, type: 2}
